<view xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:Config/etc/view.xsd">
    <media>
        <images module="Magento_Catalog">
            <image id="most_viewed_products_list" type="small_image">
                <width>140</width>
                <height>140</height>
            </image>
        </images>
    </media>
    <vars module="Magento_Catalog">
        <var name="most_viewed_products_list:type">small_image</var>
        <var name="most_viewed_products_list:width">140</var>
        <var name="most_viewed_products_list:ratio">1</var>
        <var name="most_viewed_products_list:height">140</var>
    </vars>
</view>
